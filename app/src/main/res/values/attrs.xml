<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyTitle">
        <attr name="title" format="string"></attr>
        <attr name="isBack" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="MyLoading">
        <attr name="width" format="dimension"></attr>
        <attr name="height" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="MyRefresh">
        <attr name="isNoLoad" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="MySuperItem">
        <attr name="isPadding" format="boolean"></attr>
        <!--left-left图片-->
        <attr name="left_limg" format="reference"></attr>
        <attr name="left_limg_width" format="integer"></attr>
        <attr name="left_limg_height" format="integer"></attr>
        <attr name="left_limg_leftMargin" format="integer"></attr>
        <attr name="left_limg_rightMargin" format="integer"></attr>
        <attr name="left_limg_topMargin" format="integer"></attr>
        <attr name="left_limg_bottomMargin" format="integer"></attr>
        <attr name="noLeftImageL" format="boolean"></attr>
        <!--left文字-->
        <attr name="left_text" format="string"></attr>
        <attr name="left_text_color" format="color"></attr>
        <attr name="left_text_size" format="integer"></attr>
        <attr name="left_drawable_padding" format="integer"></attr>
        <attr name="left_text_rightMargin" format="integer"></attr>
        <attr name="left_text_leftMargin" format="integer"></attr>
        <attr name="left_text_topMargin" format="integer"></attr>
        <attr name="left_text_bottomMargin" format="integer"></attr>
        <attr name="noLeftText" format="boolean"></attr>
        <!--left-right图片-->
        <attr name="left_rimg" format="reference"></attr>
        <attr name="left_rimg_width" format="integer"></attr>
        <attr name="left_rimg_height" format="integer"></attr>
        <!--right-left图片-->
        <attr name="right_rimg" format="reference"></attr>
        <attr name="right_limg_width" format="integer"></attr>
        <attr name="right_limg_height" format="integer"></attr>
        <attr name="right_limg_leftMargin" format="integer"></attr>
        <attr name="right_limg_rightMargin" format="integer"></attr>
        <attr name="right_limg_topMargin" format="integer"></attr>
        <attr name="right_limg_bottomMargin" format="integer"></attr>
        <attr name="noRightImgL" format="boolean"></attr>
        <!--right文字-->
        <attr name="right_text" format="string"></attr>
        <attr name="right_text_color" format="color"></attr>
        <attr name="right_text_size" format="integer"></attr>
        <attr name="right_drawable_padding" format="integer"></attr>
        <attr name="right_text_rightMargin" format="integer"></attr>
        <attr name="right_text_leftMargin" format="integer"></attr>
        <attr name="right_text_topMargin" format="integer"></attr>
        <attr name="right_text_bottomMargin" format="integer"></attr>
        <attr name="noRightText" format="boolean"></attr>
        <!--right-right图片-->
        <attr name="right_limg" format="reference"></attr>
        <attr name="right_rimg_width" format="integer"></attr>
        <attr name="right_rimg_height" format="integer"></attr>
        <attr name="isArrow" format="boolean"></attr>
        <!--CheckBox-->
        <attr name="isCheckBox" format="boolean"></attr>
        <attr name="checkBox_width" format="boolean"></attr>
        <attr name="checkBox_height" format="boolean"></attr>
        <attr name="checkBox_leftMargin" format="boolean"></attr>
        <attr name="checkBox_topMargin" format="integer"></attr>
        <attr name="checkBox_rightMargin" format="integer"></attr>
        <attr name="checkBox_bottomMargin" format="integer"></attr>
        <!--switch-->
        <attr name="isSwitch" format="boolean"></attr>
        <!--top-->
        <attr name="top_gravity" format="string"></attr>
        <attr name="top_topMargin" format="integer"></attr>
        <attr name="top_leftMargin" format="integer"></attr>
        <attr name="top_rightMargin" format="integer"></attr>
        <attr name="top_bottomMargin" format="integer"></attr>
        <attr name="noTop" format="boolean"></attr>
        <attr name="isTopBetween" format="boolean"></attr>
        <!--top-left文字-->
        <attr name="top_left_text" format="string"></attr>
        <attr name="top_left_textSize" format="integer"></attr>
        <attr name="top_left_textColor" format="color|reference"></attr>
        <attr name="top_left_leftMargin" format="integer"></attr>
        <attr name="top_left_topMargin" format="integer"></attr>
        <attr name="top_left_rightMargin" format="integer"></attr>
        <attr name="top_left_bottomMargin" format="integer"></attr>
        <!--top-right文字-->
        <attr name="top_right_text" format="string"></attr>
        <attr name="top_right_textSize" format="integer"></attr>
        <attr name="top_right_textColor" format="color|reference"></attr>
        <attr name="top_right_leftMargin" format="integer"></attr>
        <attr name="top_right_topMargin" format="integer"></attr>
        <attr name="top_right_rightMargin" format="integer"></attr>
        <attr name="top_right_bottomMargin" format="integer"></attr>
        <!--bottom-->
        <attr name="bottom_gravity" format="string"></attr>
        <attr name="bottom_topMargin" format="integer"></attr>
        <attr name="bottom_leftMargin" format="integer"></attr>
        <attr name="bottom_rightMargin" format="integer"></attr>
        <attr name="bottom_bottomMargin" format="integer"></attr>
        <attr name="noBottom" format="boolean"></attr>
        <attr name="isBottomBetween" format="boolean"></attr>
        <!--bottom-left文字-->
        <attr name="bottom_left_text" format="string"></attr>
        <attr name="bottom_left_textSize" format="integer"></attr>
        <attr name="bottom_left_textColor" format="color|reference"></attr>
        <attr name="bottom_left_leftMargin" format="integer"></attr>
        <attr name="bottom_left_topMargin" format="integer"></attr>
        <attr name="bottom_left_rightMargin" format="integer"></attr>
        <attr name="bottom_left_bottomMargin" format="integer"></attr>
        <!--bottom-right文字-->
        <attr name="bottom_right_text" format="string"></attr>
        <attr name="bottom_right_textSize" format="integer"></attr>
        <attr name="bottom_right_textColor" format="color"></attr>
        <attr name="bottom_right_leftMargin" format="integer"></attr>
        <attr name="bottom_right_topMargin" format="integer"></attr>
        <attr name="bottom_right_rightMargin" format="integer"></attr>
        <attr name="bottom_right_bottomMargin" format="integer"></attr>

    </declare-styleable>

</resources>